tExcel.vue"],"sourcesContent":["<template>\r\n  <div class=\"export-excel-wrapper\">\r\n    <Hints>\r\n      <template slot=\"hintName\">JS-xlsx插件</template>\r\n      <template slot=\"hintInfo\">\r\n        <p>JS-xlsx：由SheetJS出品的一款非常方便的只需要纯JS即可读取和导出excel的工具库，功能强大，支持xlsx、csv、txt等格式</p>\r\n        <p>github地址：访问 <el-link type=\"success\" href=\"https://github.com/SheetJS/sheetjs\" target=\"_blank\">JS-xlsx</el-link></p>\r\n      </template>\r\n    </Hints>\r\n    <el-card shadow=\"always\">\r\n      <el-form\r\n        ref=\"searchForm\"\r\n        :inline=\"true\"\r\n        :model=\"exportParam\"\r\n        label-width=\"100px\"\r\n        class=\"search-form\"\r\n      >\r\n        <el-form-item label=\"文件名:\">\r\n          <el-input v-model=\"exportParam.fileName\" placeholder=\"文件名\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"自动宽度:\">\r\n          <el-radio-group v-model=\"exportParam.autoWidth\">\r\n            <el-radio :label=\"true\">自动</el-radio>\r\n            <el-radio :label=\"false\">固定</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item label=\"文件类型:\">\r\n          <el-select v-model=\"exportParam.type\" placeholder=\"文件类型\">\r\n            <el-option value=\"xlsx\" label=\"xlsx\" />\r\n            <el-option value=\"csv\" label=\"csv\" />\r\n            <el-option value=\"txt\" label=\"txt\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" style=\"margin-left: 30px\" @click=\"handleExport\">导出Excel</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-table\r\n        ref=\"multipleTable\"\r\n        v-loading=\"listLoading\"\r\n        :data=\"tableData\"\r\n        tooltip-effect=\"dark\"\r\n        style=\"width: 100%\"\r\n        size=\"medium\"\r\n        @selection-change=\"handleSelectionChange\"\r\n      >\r\n        <el-table-column type=\"selection\" width=\"50\" align=\"center\" />\r\n        <el-table-column prop=\"id\" label=\"编号\" align=\"center\" />\r\n        <el-table-column prop=\"name\" label=\"姓名\" align=\"center\" />\r\n        <el-table-column prop=\"sex\" label=\"性别\" align=\"center\" />\r\n        <el-table-column prop=\"phone\" label=\"手机\" align=\"center\" />\r\n        <el-table-column prop=\"education\" label=\"学历\" align=\"center\" />\r\n        <el-table-column prop=\"hobby\" label=\"爱好\" align=\"center\" width=\"300\" />\r\n      </el-table>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getTableList } from '@/api'\r\nimport excel from '../../utils/excel'\r\nimport Hints from '../../components/Hints'\r\n\r\nexport default {\r\n  name: 'ExportExcel',\r\n  components: { Hints },\r\n  data() {\r\n    return {\r\n      listLoading: true,\r\n      exportParam: {\r\n        fileName: '',\r\n        autoWidth: true,\r\n        type: 'xlsx'\r\n      },\r\n      tableData: [],\r\n      multipleSelection: []\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchData()\r\n  },\r\n  methods: {\r\n    fetchData() {\r\n      this.listLoading = true\r\n      getTableList(this.exportParam).then(res => {\r\n        const data = res.data\r\n        if (data.code === 0) {\r\n          data.data.list.forEach(item => {\r\n            const { id, name, sex, phone, education, hobby } = item\r\n            this.tableData.push({ id, name, sex, phone, education, hobby })\r\n          })\r\n          this.listLoading = false\r\n        }\r\n      }).catch(() => {\r\n        this.listLoading = false\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = val\r\n    },\r\n    handleExport() {\r\n      if (this.multipleSelection.length) {\r\n        const params = {\r\n          header: ['编号', '姓名', '性别', '手机', '学历', '爱好'],\r\n          key: ['id', 'name', 'sex', 'phone', 'education', 'hobby'],\r\n          data: this.multipleSelection,\r\n          autoWidth: this.exportParam.autoWidth,\r\n          fileName: this.exportParam.fileName,\r\n          bookType: this.exportParam.type\r\n        }\r\n        excel.exportDataToExcel(params)\r\n        this.$refs.multipleTable.clearSelection()\r\n      } else {\r\n        this.$message.warning('请勾选要导出的数据项！')\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.export-excel-wrapper {\r\n  .el-card {\r\n    min-height: 500px;\r\n  }\r\n  .search-form {\r\n    padding-top: 18px;\r\n    margin-bottom: 15px;\r\n    background-color: #f7f8fb;\r\n  }\r\n  .el-table thead {\r\n    font-weight: 600;\r\n    th{\r\n      background-color: #f2f3f7;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;;AA2DA,IAAAA,IAAA,GAAAC,OAAA;AACA,IAAAC,MAAA,GAAAC,sBAAA,CAAAF,OAAA;AACA,IAAAG,MAAA,GAAAD,sBAAA,CAAAF,OAAA;AAAA,IAAAI,QAAA,GAAAC,OAAA,CAAAC,OAAA,GAEA;EACAC,IAAA;EACAC,UAAA;IAAAC,KAAA,EAAAA;EAAA;EACAC,KAAA;IACA;MACAC,WAAA;MACAC,WAAA;QACAC,QAAA;QACAC,SAAA;QACAC,IAAA;MACA;MACAC,SAAA;MACAC,iBAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAD,UAAA;MACA,KAAAR,WAAA;MACA,IAAAU,iBAAA,OAAAT,WAAA,EAAAU,IAAA,CAAAC,GAAA;QACA,MAAAb,IAAA,GAAAa,GAAA,CAAAb,IAAA;QACA,IAAAA,IAAA,CAAAc,IAAA;UACAd,IAAA,CAAAA,IAAA,CAAAe,IAAA,CAAAC,OAAA,CAAAC,IAAA;YACA;cAAAC,EAAA;cAAArB,IAAA;cAAAsB,GAAA;cAAAC,KAAA;cAAAC,SAAA;cAAAC;YAAA,IAAAL,IAAA;YACA,KAAAX,SAAA,CAAAiB,IAAA;cAAAL,EAAA;cAAArB,IAAA;cAAAsB,GAAA;cAAAC,KAAA;cAAAC,SAAA;cAAAC;YAAA;UACA;UACA,KAAArB,WAAA;QACA;MACA,GAAAuB,KAAA;QACA,KAAAvB,WAAA;MACA;IACA;IACAwB,sBAAAC,GAAA;MACA,KAAAnB,iBAAA,GAAAmB,GAAA;IACA;IACAC,aAAA;MACA,SAAApB,iBAAA,CAAAqB,MAAA;QACA,MAAAC,MAAA;UACAC,MAAA;UACAC,GAAA;UACA/B,IAAA,OAAAO,iBAAA;UACAH,SAAA,OAAAF,WAAA,CAAAE,SAAA;UACAD,QAAA,OAAAD,WAAA,CAAAC,QAAA;UACA6B,QAAA,OAAA9B,WAAA,CAAAG;QACA;QACA4B,cAAA,CAAAC,iBAAA,CAAAL,MAAA;QACA,KAAAM,KAAA,CAAAC,aAAA,CAAAC,cAAA;MACA;QACA,KAAAC,QAAA,CAAAC,OAAA;MACA;IACA;EACA;AACA","ignoreList":[]}]}