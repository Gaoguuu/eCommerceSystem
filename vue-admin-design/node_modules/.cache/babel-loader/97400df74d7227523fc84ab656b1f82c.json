ndleLogin() {\r\n      this.$refs.loginForm.validate(valid => {\r\n        const data = {\r\n          username: this.loginForm.username,\r\n          password: this.loginForm.password\r\n        }\r\n        if (valid) {\r\n          this.loading = true\r\n          login(data).then(res => {\r\n            this.loading = false\r\n            setToken(res.token)\r\n            this.$router.push({ path: this.redirect || '/' })\r\n          }).catch(() => {\r\n            this.loading = false\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.login-wrapper {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100%;\r\n  height: 100vh;\r\n  background-size: cover;\r\n  .form-box {\r\n    width: 320px;\r\n    padding: 16px 30px;\r\n    background: #fff;\r\n    border-radius: 4px;\r\n    box-shadow: 0 15px 30px 0 rgba(0, 0, 1, .1);\r\n    .form-title {\r\n      margin: 0 auto 35px;\r\n      text-align: center;\r\n      color: #707070;\r\n      font-size: 18px;\r\n      letter-spacing: 2px;\r\n\r\n      img {\r\n        width: 200px;\r\n        height: 82px;\r\n        margin: 14px auto 0;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;;AA6BA,IAAAA,MAAA,GAAAC,OAAA;AACA,IAAAC,OAAA,GAAAD,OAAA;AACA,IAAAE,gBAAA,GAAAC,sBAAA,CAAAH,OAAA;AAAA,IAAAI,QAAA,GAAAC,OAAA,CAAAC,OAAA,GAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,UAAA,EAAAA,wBAAA;MACAC,SAAA;QACAC,QAAA;QACAC,QAAA;QACAC,UAAA;MACA;MACAC,UAAA;QACAH,QAAA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAL,QAAA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAC,OAAA;MACAC,QAAA,EAAAC;IACA;EACA;EACAC,KAAA;IACAC,MAAA;MACAC,OAAA,WAAAA,CAAAC,KAAA;QACA,KAAAL,QAAA,GAAAK,KAAA,CAAAC,KAAA,IAAAD,KAAA,CAAAC,KAAA,CAAAN,QAAA;MACA;MACAO,SAAA;IACA;EACA;EACAC,OAAA;IACAC,YAAA;MACA,KAAAC,KAAA,CAAAnB,SAAA,CAAAoB,QAAA,CAAAC,KAAA;QACA,MAAAvB,IAAA;UACAG,QAAA,OAAAD,SAAA,CAAAC,QAAA;UACAC,QAAA,OAAAF,SAAA,CAAAE;QACA;QACA,IAAAmB,KAAA;UACA,KAAAb,OAAA;UACA,IAAAc,