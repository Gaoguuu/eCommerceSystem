n\r\n    }\r\n    config.headers['Content-Type'] = 'application/json' // 请求的数据格式为 json\r\n    return config\r\n  },\r\n  error => {\r\n    console.log(error)\r\n    Promise.reject(error)\r\n  }\r\n)\r\n\r\n// response 拦截器设置\r\nservice.interceptors.response.use(\r\n  response => {\r\n    const data = response.data\r\n    const code = data.code\r\n    const message = data.msg\r\n    if (code === 0) {\r\n      return data.data\r\n    } else {\r\n      Message({\r\n        type: 'error',\r\n        message\r\n      })\r\n      return Promise.reject('error')\r\n    }\r\n  },\r\n  error => {\r\n    const code = error.response.data.status\r\n    if (!code) {\r\n      Message({\r\n        type: 'error',\r\n        message: '接口请求失败'\r\n      })\r\n    }\r\n    return Promise.reject(error)\r\n  }\r\n)\r\n\r\nexport default service\r\n"],"mappings":";;;;;;;AAAA,IAAAA,MAAA,GAAAC,sBAAA,CAAAC,OAAA;AACA,IAAAC,UAAA,GAAAD,OAAA;AACA,IAAAE,OAAA,GAAAF,OAAA;AAEA;AACA,MAAMG,OAAO,GAAGC,cAAK,CAACC,MAAM,CAAC;EAC3BC,OAAO,EAAEC,OAAO,CAACC,GAAG,CAACC,gBAAgB;EAAE;EACvCC,OAAO,EAAE,MAAM,CAAC;AAClB,CAAC,CAAC;;AAEF;AACAP,OAAO,CAACQ,YAAY,CAACC,OAAO,CAACC,GAAG,CAC9BC,MAAM,IAAI;EACR,IAAI,IAAAC,gBAAQ,EAAC,CAAC,EAAE;IACdD,MAAM,CAACE,OAAO,CAAC,eAAe,CAAC,GAAG,SAAS,GAAG,IAAAD,gBAAQ,EAAC,CAAC,EAAC;EAC3D;EACAD,MAAM,CAACE,OAAO,CAAC,cAAc,CAAC,GAAG,kBAAkB,EAAC;EACpD,OAAOF,MAAM;AACf,CAAC,EACDG,KAAK,IAAI;EACPC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;EAClBG,OAAO,CAACC,MAAM,CAACJ,KAAK,CAAC;AACvB,CACF,CAAC;;AAED;AACAd,OAAO,CAACQ,YAAY,CAACW,QAAQ,CAACT,GAAG,CAC/BS,QAAQ,IAAI;EACV,MAAMC,IAAI,GAAGD,QAAQ,CAACC,IAAI;EAC1B,MAAMC,IAAI,GAAGD,IAAI,CAACC,IAAI;EACtB,MAAMC,OAAO,GAAGF,IAAI,CAACG,GAAG;EACxB,IAAIF,IAAI,KAAK,CAAC,EAAE;IACd,OAAOD,IAAI,CAACA,IAAI;EAClB,CAAC,MAAM;IACL,IAAAI,kBAAO,EAAC;MACNC,IAAI,EAAE,OAAO;MACbH;IACF,CAAC,CAAC;IACF,OAAOL,OAAO,CAACC,MAAM,CAAC,OAAO,CAAC;EAChC;AACF,CAAC,EACDJ,KAAK,IAAI;EACP,MAAMO,IAAI,GAAGP,KAAK,CAACK,QAAQ,CAACC,IAAI,CAACM,MAAM;EACvC,IAAI,CAACL,IAAI,EAAE;IACT,IAAAG,kBAAO,EAAC;MACNC,IAAI,EAAE,OAAO;M